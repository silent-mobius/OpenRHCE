[project]
	description = "An effort to collaboratively prepare study materials to help candidates achieve Red Hat's RHCE certification.  Please note that this effort will be based ONLY on Red Hat's posted objectives and NOT on any insider knowledge of the actual contents of the exam.  This is not an effort to provide "
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/silent-mobius (silent-mobius)
[access "refs/for/refs/*"]
	read = group user/silent-mobius (silent-mobius)
	push = group user/silent-mobius (silent-mobius)
	label-Code-Review = -2..+2 group user/silent-mobius (silent-mobius)
	label-Verified = -1..+1 group user/silent-mobius (silent-mobius)
	rebase = group user/silent-mobius (silent-mobius)
	removeReviewer = group user/silent-mobius (silent-mobius)
	submit = group user/silent-mobius (silent-mobius)
[access "refs/heads/*"]
	read = group user/silent-mobius (silent-mobius)
	create = group user/silent-mobius (silent-mobius)
	push = +force group user/silent-mobius (silent-mobius)
	pushMerge = group user/silent-mobius (silent-mobius)
[access "refs/tags/*"]
	push = group user/silent-mobius (silent-mobius)
	push = +force group user/silent-mobius (silent-mobius)
